import numpy as np

def hill_encrypt(plaintext, key_matrix):
    n = key_matrix.shape[0]
    plaintext_numbers = [ord(char) - ord('A') for char in plaintext]
    plaintext_vector = np.array(plaintext_numbers).reshape(-1, n)
    ciphertext_vector = (plaintext_vector @ key_matrix) % 26
    ciphertext = ''.join(chr(num + ord('A')) for num in ciphertext_vector.flatten())
    return ciphertext

plaintext = "HELLO"
key_matrix = np.array([[3, 3], [2, 5]])
ciphertext = hill_encrypt(plaintext, key_matrix)
print("Ciphertext:", ciphertext)
